    "def match(Sstring,Tstring):\n",
    "    count=0\n",
    "    if Tstring=='':\n",
    "        return 1\n",
    "    for i in range(len(Sstring)):\n",
    "        if Sstring[i]==Tstring[0]:\n",
    "            count+=match(Sstring[i+1:],Tstring[1:])\n",
    "    return count\n",
    "\n",
    "print( match('dacarcar','car'))"
   ]
